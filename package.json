    "ip": "^2.0.1",
    "lodash.get": "^4.4.2",
    "lru-cache": "^10.2.0",
    "pacote": "^17.0.6",
    "progress": "^2.0.3",
    "semver": "^7.6.0",
    "semver-utils": "^1.1.4"
  },
  "devDependencies": {
    "c8": "^9.1.0",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "mocha": "^10.3.0",
    "npm-run-all2": "^6.1.2",
    "rimraf": "^5.0.5",
    "shelljs": "^0.8.5"
